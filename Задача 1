#include <stdio.h>
2	#include <string.h>
3	
4	// Function to find the maximum of two integers
5	int max(int a, int b) {
6	    if (a > b) {
7	        return a;
8	    } else {
9	        return b;
10	    }
11	    // Or, more concisely: return (a > b) ? a : b;
12	}
13	
14	// Main function: entry point of the program
15	int main() {
16	    int num1 = 10;
17	    int num2 = 20;
18	    
19	    // Call the max function and print the result
20	    int result = max(num1, num2);
21	    printf("Максимальное число это: %d\n", result); // Display the maximum number
22	
23	    return 0; 
24	}
