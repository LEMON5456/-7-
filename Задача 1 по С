#include <stdio.h>
	#include <string.h>

	// Function to find the maximum of two integers
	int max(int a, int b) {
    if (a > b) {
	        return a;
    } else {
        return b;
	    }
	    // Or, more concisely: return (a > b) ? a : b;
	}
	
	// Main function: entry point of the program
	int main() {
    int num1 = 10;
    int num2 = 20;
	    
	    // Call the max function and print the result
	    int result = max(num1, num2);
    printf("Максимальное число это: %d\n", result); // Display the maximum number

    return 0; 
}
